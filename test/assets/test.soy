{namespace My.Namespace}

/**
 * @param firstName
 */
{template .hello}
	{@param lastName: string /}
	{@param? title: string|html /}
	Hello{$title ? ' ' + $title : ''} {$firstName} {$lastName}!
{/template}

/**
 */
{template .age private="true"   autoescape="false" }
	{@param age: number }
	Your age: {$age}.
{/template}

/**
 * @param id
 */
{deltemplate Del}
	My id is {$id}.
{/deltemplate}

/**
 * @param id
 */
{deltemplate Del variant="'negation'"}
	My id is not {$id}.
{/deltemplate}
